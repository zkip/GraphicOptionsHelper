type Menu {
	items	[]string
	current	uint

	frag {
		ul {
			@if items.length > 3 {
				@for items as item, idx {
					li	(
						tag = idx == current ? "current" : ""
						) {
						span .label item
					}
				}
			} else {
				span "..."
			}
		}
	}
}

